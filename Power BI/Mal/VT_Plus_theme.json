{
    "name": "VT+",
    "dataColors": [
        "#7DBEBA",
        "#CFC98B",
        "#EB8380",
        "#AD879E",
        "#87A5AF",
        "#8DB9A0",
        "#D1AB94",
        "#7ABED3"
    ],
    "minimum": "#D1EAE9",
    "center": "#699EA5",
    "maximum": "#005260",
    "firstLevelElements": "#605E5C",
    "secondLevelElements": "#605E5C",
    "thirdLevelElements": "#F3F2F1",
    "fourthLevelElements": "#B3B0AD",
    "textClasses": {
        "callout": {
            "fontSize": 42,
            "fontFace": "DIN",
            "color": "#605E5C"
        },
        "title": {
            "fontSize": 16,
            "fontFace": "DIN",
            "color": "#605E5C"
        },
        "label": {
            "fontSize": 16,
            "fontFace": "Segoe UI"
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "*": [
                    {
                        "fontSize": 16,
                        "color": {
                            "solid": {}
                        }
                    }
                ],
                "general": [
                    {
                        "responsive": true
                    }
                ],
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FBFBFB"
                            }
                        },
                        "transparency": 30
                    }
                ],
                "border": [
                    {
                        "color": {
                            "solid": {
                                "color": "#CACACA"
                            }
                        },
                        "show": true,
                        "radius": 1
                    }
                ],
                "title": [
                    {
                        "show": false
                    }
                ],
                "visualTooltip": [
                    {
                        "titleFontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "valueFontColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ],
                "outspacePane": [
                    {
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ]
            }
        },
        "basicShape": {
			"*": {
				"border": 
                    [{
					"show": false
				    }]
			    }
		    },
        "actionButton": {
            "*": {
                "border":
                    [{
                        "show": false
                    }],
                "visualHeader":
                    [{
                        "show":false
                    }],
                "background":
                    [{
                        "show":false
                    }],
                "title":
                    [{
                        "show":false
                    }],
                "outline":
                    [
                        {
                            "show":true
                        },
                        {
                            "$id": "default",
                            "weight":2,
                            "radius":4
                        },
                        {
                            "$id": "hover",
                            "weight":2,
                            "radius":4
                        },
                        {
                            "$id": "selected",
                            "weight":2,
                            "radius":4
                        }
                    ],
                "fill":
                    [
                        {
                            "show":true
                        },
                        {
                            "$id": "default",
                            "transparency": 100,
                            "fillColor": {"solid":{"color":"#CACACA"}}
                        },
                        {
                            "$id": "hover",
                            "transparency": 0,
                            "fillColor": {"solid":{"color":"#CACACA"}}
                        },
                        {
                            "$id": "selected",
                            "transparency": 100,
                            "fillColor": {"solid":{"color":"#CACACA"}}
                        }
                    ]
                }
            },            
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#EEEEEE"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "outspace": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ]
            }
        },
        "lineChart": {
			"*": {
                "title": 
                    [{
					"show": true,
                    "alignment": "center"
				    }],
				"legend": 
                    [{
					"show": true,
                    "showTitle": false,
                    "position": "RightCenter"
				    }],
                "categoryAxis": 
                    [{
					"showAxisTitle": false
				    }],
                "valueAxis": 
                    [{
					"showAxisTitle": false,
                    "labelDisplayUnits": "1",
                    "labelPrecision": "Auto"
				    }],
                "lineStyles": 
                    [{
					"strokeWidth": 4
				    }]
			    }
		    },
        "textbox": {
            "*": {
                "*": [
                    {
                        "fontSize": 16,
                        "fontFamily": "Calibri",
                        "wordWrap": true,
                        "color": {
                            "solid": {
                                "color": "#666666"
                            }
                        }
                    }
                ],
                "general": [
                    {
                        "responsive": true
                    }
                ],
                "background": [
                    {
                        "show": false
                    }
                ],
                "visualHeader": [
                    {
                        "show": true
                    }
                ],
                "border": [
                    {
                        "show": false
                    }
                ],
                "title": [
                    {
                        "fontFamily": "Calibri",
                        "fontSize": 18
                    }
                ]
            }
        },
        "shapeMap": {
            "*": {
				"background": [
                    {
				        "show": false
				    }
                ]
            }        
        },
        "image": {
            "*": {
				"border": [
                    {
				        "show": false
				    }
                ]
            }        
        },
        "clusteredBarChart": {
            "*": {
				"valueAxis": [
                    {
				        "showAxisTitle": true,
                        "labelDisplayUnits": "1",
                        "labelPrecision": "Auto"
				    }
                ],
                "categoryAxis": [
                    {
				        "showAxisTitle": false
				    }
                ]
            }        
        },
        "card": {
			"*": {
				"title": [
                    {
				        "show": true,
				        "fontSize": 28,
                        "fontColor": { "solid": { "color": "#333333" } },
                        "alignment": "center"
				    }
                ],
                "labels": [
                    {
                        "labelDisplayUnits": "1",
                        "labelPrecision": "Auto",
                        "fontSize": 42,
                        "color": { "solid": { "color": "#333333"}}
                    }
                ],
                "background": [
                    {
                        "show": true,
                        "color": {
                            "solid": {
                                "color": "#D1EAE9"
                            }
                    },
                        "transparency": 0
                    }
                ],
                "categoryLabels": [
                    {
                        "fontSize": 20,
                        "color": { "solid": { "color": "#333333"}}
                    }
                ],
                "border": [
                    {
                        "show": true,
                        "color": "#CACACA",
                        "radius": 1
                    }
                ]
			}
        }           
    }
}